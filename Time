class Time:
    def __init__(self):
        runtime = 0
        pass
    
    def __str__(self, runtime):
        return datetime.timedelta(seconds=self.runtime)

    def start(self):
        """Starts the runtime"""

    def pause(self):
        """Pauses the runtime"""

    def stop(self):
        """Stops the runtime"""
        return self.runtime

